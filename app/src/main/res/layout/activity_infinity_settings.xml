<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/layout_sensors"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/navigation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:context="me.aflak.motorica.ui.scan.view.ScanActivity"
        tools:ignore="MissingConstraints,UnknownIdInLayout">

        <TextView
            android:id="@+id/valueTimeClose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="50dp"
            android:layout_marginTop="49dp"
            android:text="1000"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:typeface="monospace"
            app:fontFamily="sans-serif-medium"
            tools:ignore="HardcodedText" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="70dp"
            android:background="@android:color/darker_gray" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="137dp"
            android:background="@android:color/darker_gray" />

        <Button
            android:id="@+id/save_gripper_settings"
            android:layout_width="206dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="60dp"
            android:layout_marginLeft="60dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginBottom="16dp"
            android:text="Сохранить"
            android:visibility="visible"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_marginStart="0dp"
            android:layout_marginTop="4dp"
            android:text="Time:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="72dp"
            android:text="Speed:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignStart="@+id/chartCH1"
            android:layout_marginStart="0dp"
            android:layout_marginTop="27dp"
            android:text="t откр:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="94dp"
            android:text="v откр:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignStart="@+id/chartCH1"
            android:layout_marginStart="0dp"
            android:layout_marginTop="49dp"
            android:text="t закр:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="116dp"
            android:text="v закр:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="141dp"
            android:text="Количество циклов:"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignStart="@+id/textView13"
            android:layout_marginStart="0dp"
            android:layout_marginTop="27dp"
            android:text="ms"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="89dp"
            android:layout_marginTop="116dp"
            android:text="rad/s"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView14"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="93dp"
            android:text="rad/s"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="89dp"
            android:layout_marginTop="49dp"
            android:text="ms"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/valueTimeOpen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="50dp"
            android:layout_marginTop="27dp"
            android:text="1000"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:typeface="monospace"
            app:fontFamily="sans-serif-medium"
            tools:ignore="HardcodedText" />

        <SeekBar
            android:id="@+id/seekBarTimeOpen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignStart="@+id/seekBarTimeClose"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:max="2000"
            android:progress="1000" />

        <SeekBar
            android:id="@+id/seekBarSpeedClose"
            android:layout_width="161dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="4dp"
            android:layout_marginTop="116dp"
            android:layout_marginEnd="0dp"
            android:layout_toEndOf="@+id/textView14"
            android:max="999"
            android:progress="999" />

        <SeekBar
            android:id="@+id/seekBarTimeClose"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/chartCH1"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="4dp"
            android:layout_marginTop="49dp"
            android:layout_marginEnd="0dp"
            android:layout_toEndOf="@+id/textView13"
            android:max="2000"
            android:progress="1000" />

        <SeekBar
            android:id="@+id/seekBarSpeedOpen"
            android:layout_width="185dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/seekBarSpeedClose"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="95dp"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:max="999"
            android:progress="999" />

        <TextView
            android:id="@+id/valueSpeedOpen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="50dp"
            android:layout_marginTop="94dp"
            android:text="0.999"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:typeface="monospace"
            app:fontFamily="sans-serif-medium"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/valueFinger2Angle2"
            android:layout_width="239dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_gravity="right"
            android:layout_marginStart="6dp"
            android:layout_marginTop="141dp"
            android:layout_marginEnd="0dp"
            android:layout_toEndOf="@+id/textView21"
            android:layout_toRightOf="@+id/textView21"
            android:text="0"
            android:textStyle="bold"
            app:fontFamily="sans-serif-medium"
            tools:ignore="HardcodedText,RtlHardcoded" />

        <TextView
            android:id="@+id/valueSpeedClose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="50dp"
            android:layout_marginTop="116dp"
            android:text="0.999"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:typeface="monospace"
            app:fontFamily="sans-serif-medium"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>