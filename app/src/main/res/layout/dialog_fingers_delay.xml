<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/cv"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gl_andex_alert_dialog_layout_image_left1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.05" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gl_andex_alert_dialog_layout_image_right2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.95" />

            <TextView
                android:id="@+id/dialog_fingers_delay_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/sf_pro_text_bold"
                android:gravity="center"
                android:maxLines="2"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="Title dialog info"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/gl_andex_alert_dialog_layout_image_right2"
                app:layout_constraintStart_toStartOf="@+id/gl_andex_alert_dialog_layout_image_left1"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText" />

            <androidx.cardview.widget.CardView
                android:id="@+id/all_fingers_delay_cv"
                android:layout_width="0dp"
                android:layout_height="383dp"
                android:layout_marginTop="16dp"
                android:elevation="0dp"
                app:cardCornerRadius="16dp"
                app:cardUseCompatPadding="true"
                app:layout_constraintEnd_toStartOf="@+id/gl_andex_alert_dialog_layout_image_right2"
                app:layout_constraintStart_toStartOf="@+id/gl_andex_alert_dialog_layout_image_left1"
                app:layout_constraintTop_toBottomOf="@+id/dialog_fingers_delay_title_tv">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/separator_settings"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toStartOf="@+id/gl_andex_alert_dialog_layout_image_right2"
                    app:layout_constraintStart_toStartOf="@+id/gl_andex_alert_dialog_layout_image_left1"
                    app:layout_constraintTop_toBottomOf="@+id/dialog_fingers_delay_title_tv">

                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/delay_fingers_animation_view"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        tools:ignore="ImageContrastCheck" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/first_cl"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginBottom="2dp"
                        android:background="@color/background_title_fragment">

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_1_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.33" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_2_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.50" />

                        <TextView
                            android:id="@+id/dialog_fingers_delay_first_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="@string/_1_finger_delay"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_1_1"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/dialog_fingers_delay_first_2_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="123 ms"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_2_1"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="@+id/_1_1"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="HardcodedText" />

                        <androidx.appcompat.widget.AppCompatSeekBar
                            android:id="@+id/dialog_fingers_delay_1_sb"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:max="100"
                            android:paddingStart="8dp"
                            android:layout_marginEnd="8dp"
                            android:paddingEnd="0dp"
                            android:progress="0"
                            android:progressDrawable="@drawable/track_le"
                            android:thumb="@drawable/thumb_le"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/_2_1"
                            tools:layout_editor_absoluteY="0dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/second_cl"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginBottom="2dp"
                        android:background="@color/background_title_fragment">

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_1_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.33" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_2_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.5" />

                        <TextView
