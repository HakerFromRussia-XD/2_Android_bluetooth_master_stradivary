                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="4dp" />

                <TextView
                    android:id="@+id/valueInvert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/seekBarRoughness"
