                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="@string/_4_finger_delay"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_1_4"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dialog_fingers_delay_fourth_2_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="123 ms"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_2_4"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="@+id/_1_4"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="HardcodedText" />

                        <androidx.appcompat.widget.AppCompatSeekBar
                            android:id="@+id/dialog_fingers_delay_4_sb"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:max="100"
                            android:paddingStart="8dp"
                            android:layout_marginEnd="8dp"
                            android:paddingEnd="0dp"
                            android:progress="0"
                            android:progressDrawable="@drawable/track_le"
                            android:thumb="@drawable/thumb_le"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/_2_4"
                            tools:layout_editor_absoluteY="0dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/fifth_cl"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginBottom="2dp"
                        android:background="@color/background_title_fragment">

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_1_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.33" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_2_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.5" />

                        <TextView
                            android:id="@+id/dialog_fingers_delay_fifth_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="@string/_5_finger_delay"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_1_5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dialog_fingers_delay_fifth_2_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/sf_pro_display_light"
                            android:gravity="center_vertical"
                            android:maxLines="3"
                            android:paddingStart="10dp"
                            android:paddingTop="2dp"
                            android:paddingBottom="4dp"
                            android:singleLine="false"
                            android:text="123 ms"
                            android:textColor="#000"
                            android:textSize="12sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/_2_5"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="@+id/_1_5"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="HardcodedText" />

                        <androidx.appcompat.widget.AppCompatSeekBar
                            android:id="@+id/dialog_fingers_delay_5_sb"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:max="100"
                            android:paddingStart="8dp"
                            android:layout_marginEnd="8dp"
                            android:paddingEnd="0dp"
                            android:progress="0"
                            android:progressDrawable="@drawable/track_le"
                            android:thumb="@drawable/thumb_le"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/_2_5"
                            tools:layout_editor_absoluteY="0dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/sixth_cl"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginBottom="2dp"
                        android:background="@color/background_title_fragment">

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_1_6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.33" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/_2_6"
